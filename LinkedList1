import java.util.*;

//LinkedList class (Collections Framework)
public class LL1 {
    public static void main(String[] args) {
        LinkedList<String> list = new LinkedList<String>();
        // add - first
        list.addFirst("a");
        list.addFirst("is");
        list.addFirst("this");
        System.out.println(list);

        // add - last
        list.addLast("list");
        System.out.println(list);

        // size
        System.out.println(list.size());

        // loop
        for (int i = 0; i < list.size(); i++) {
            System.out.print(list.get(i) + "->");
        }
        System.out.println("null");

        // delete - first
        list.removeFirst();
        System.out.println(list);

        // delete - last
        list.removeLast();
        System.out.println(list);

        // delete at index
        list.remove(1);
        System.out.println(list);
    }
}
